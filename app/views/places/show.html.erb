<h1><%= @place["name"] %></h1>

<% for post in @posts %>
<li>
<h2> <%= post["title"] %> </h2>
<h3> <%= post["posted_on"] %> </h3>
<%= post["description"] %>
</li>
<% end %>

<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>

<p>
    <a href="/places"> Back to Places I've Been </a>
</p>